package entities;

import java.util.Date;

import entities.enums.OrderStatus;

public class Order {

	private Integer id;	// começa como nulo, pode ser instanciado numa lista
	private Date moment;
	private OrderStatus status;
	
	public Order() {}

	public Order(Integer id, Date moment, OrderStatus status) {
		this.id = id;
		this.moment = moment;
		this.status = status;
	}
	
	
}
